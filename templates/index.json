    "custom_liquid_qDgti9": {
      "type": "custom-liquid",
      "disabled": true,
      "name": "Home Counter",
      "settings": {
        "custom_liquid": "<div class=\"banner\">\n  <div class=\"banner-text\">\n    <h3 style=\"margin: 0;\">All Brands Discount</h3>\n    <p style=\"margin: 0;\">up to 30% discounts on all brands, only on OZYRA. Hurry up!</p>\n  </div>\n  <div class=\"countdown-container\">\n    <div class=\"countdown\" id=\"countdown\">\n      <div class=\"countdown-item\">\n        <div class=\"countdown-time\" id=\"days\">00</div>\n        <div class=\"countdown-label\">Days</div>\n      </div>\n      <div class=\"countdown-item\">\n        <div class=\"countdown-time\" id=\"hours\">00</div>\n        <div class=\"countdown-label\">Hours</div>\n      </div>\n      <div class=\"countdown-item\">\n        <div class=\"countdown-time\" id=\"minutes\">00</div>\n        <div class=\"countdown-label\">Minutes</div>\n      </div>\n      <div class=\"countdown-item\">\n        <div class=\"countdown-time\" id=\"seconds\">00</div>\n        <div class=\"countdown-label\">Seconds</div>\n      </div>\n    </div>\n\n    <div class=\"countdown-message\" id=\"countdown-message\">Ops! The Sale Is Over</div>\n  </div>\n</div>\n\n<style>\n  .banner {\n    display: flex;\n    justify-content: space-between;\n    margin: 0;\n    padding: 1rem 3rem 0 3rem;\n    background-color: #ffefd5;\n  }\n\n  .banner-text {\n    margin: auto 0;\n  }\n\n  @media (width <= 40rem) {\n    .banner {\n      padding: 0.5rem 1.5rem 0 1.5rem;\n      flex-direction: column;\n      justify-content: start;\n      align-items: center;\n      gap: 0.75rem;\n    }\n\n    .countdown-container {\n      width: 100%;\n    }\n\n    .countdown-time {\n      font-weight: 550;\n      font-size: 1rem;\n    }\n\n    .countdown-label {\n      font-size: 0.5rem !important;\n    }\n  }\n\n  .countdown-container {\n    font-family: var(--font-body-family), -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;\n    /* padding: 1.5rem; */\n    /* background: linear-gradient(145deg, #f0f0f0, #ffffff); */\n    border-radius: 16px;\n    transition: all 0.3s ease;\n    perspective: 1000px;\n  }\n\n  .countdown-container:hover {\n    transform: translateY(-5px) rotateX(5deg);\n  }\n\n  .countdown {\n    display: flex;\n    justify-content: space-around;\n    flex-wrap: wrap;\n    gap: 1rem;\n  }\n\n  .countdown-item {\n    width: 4rem !important;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    transform-style: preserve-3d;\n    transition: transform 0.3s ease;\n  }\n\n  .countdown-item:hover {\n    transform: translateZ(10px) scale(1.05);\n  }\n\n  .countdown-item:hover .countdown-time {\n    transform: translateZ(15px) scale(1.1);\n    box-shadow: 0 10px 25px rgba(0, 0, 0, 0.1);\n  }\n\n  .countdown-item:hover .countdown-label {\n    transform: translateZ(5px);\n    color: #444;\n  }\n\n  .countdown-time {\n    font-size: 1.25rem !important;\n    font-weight: 700;\n    color: #000000;\n    padding: 0.5rem;\n    margin-bottom: 0.35rem;\n    position: relative;\n    transition: all 0.3s ease;\n    text-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    border-radius: 8px;\n    width: 100%;\n    text-align: center;\n  }\n\n  .countdown-time::before {\n    content: '';\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    background: rgba(255, 255, 255, 0.1);\n    border-radius: 8px;\n    transform: translateZ(-5px);\n    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1), 0 3px 5px rgba(0, 0, 0, 0.12);\n    z-index: -1;\n  }\n\n  .countdown-label {\n    font-size: 0.75rem !important;\n    color: #666;\n    text-transform: uppercase;\n    letter-spacing: 1px;\n    font-weight: 500;\n    transition: all 0.3s ease;\n    margin-top: 0.35rem;\n  }\n\n  .countdown-message {\n    display: none;\n    text-align: center;\n    font-size: 1.65rem;\n    font-weight: 600;\n    margin-top: 2.8rem;\n    color: #e74c3c;\n    animation: pulse 2s infinite;\n    text-shadow: 0 1px 3px rgba(231, 76, 60, 0.3);\n  }\n\n  @keyframes pulse {\n    0% {\n      transform: scale(1);\n      opacity: 1;\n    }\n    50% {\n      transform: scale(1.05);\n      opacity: 0.8;\n    }\n    100% {\n      transform: scale(1);\n      opacity: 1;\n    }\n  }\n\n  @media screen and (max-width: 767px) {\n    .countdown {\n      gap: 0.5rem;\n    }\n\n    .countdown-item {\n      width: 45px;\n    }\n\n    .countdown-time {\n      font-size: 1.98rem;\n    }\n\n    .countdown-label {\n      font-size: 1.1rem;\n    }\n  }\n</style>\n\n<script>\n  document.addEventListener('DOMContentLoaded', function () {\n    // Set countdown for 3 days from now\n    const countDownDate = new Date(2025, 11, 25);\n    \n    // For testing purposes, uncomment to set a shorter time (10 seconds)\n    // const countDownDate = new Date(new Date().getTime() + 10000);\n\n    const countdownTimer = setInterval(function () {\n      const now = new Date().getTime();\n      const distance = countDownDate - now;\n\n      // If the countdown is over\n      if (distance < 0) {\n        clearInterval(countdownTimer);\n        document.getElementById('countdown').style.display = 'none';\n        document.getElementById('countdown-message').style.display = 'block';\n        return;\n      }\n\n      // Time calculations\n      const days = Math.floor(distance / (1000 * 60 * 60 * 24));\n      const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\n      const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));\n      const seconds = Math.floor((distance % (1000 * 60)) / 1000);\n\n      // Update the display\n      document.getElementById('days').innerHTML = days.toString().padStart(2, '0');\n      document.getElementById('hours').innerHTML = hours.toString().padStart(2, '0');\n      document.getElementById('minutes').innerHTML = minutes.toString().padStart(2, '0');\n      document.getElementById('seconds').innerHTML = seconds.toString().padStart(2, '0');\n    }, 1000);\n  });\n</script>",
